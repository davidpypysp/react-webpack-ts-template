{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "ES6",
        "jsx": "react",
        "allowJs": true,
        "removeComments": false,
        "lib": [
            "WebWorker",
            "ES6",
            "DOM",
            "ES2020"
        ],
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "baseUrl": "./"
    }
}